{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      "**/*.proto",
      "**/*.xlsx",
      "**/*.json",
      "**/*.pem",
      "**/*.hbs"
    ],
    "watchAssets": true
  }
}